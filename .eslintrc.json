{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "no-unused-vars": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "max-len": "off",
        "no-undef": "error"
    }
}